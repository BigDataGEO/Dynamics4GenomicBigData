journal = {The Journal of general psychology},
keywords = {Adult,Bias (Epidemiology),Cognition,Factor Analysis, Statistical,Female,Humans,Intelligence,Judgment,Male,New South Wales,Perception,Personality,Reproducibility of Results,Self-Assessment,Texas},
month = {jul},
