@article{McNamara1985b,
abstract = {Optimal foraging theory usually assumes that certain key environmental parameters are known to a foraging animal, and predicts the animal's behaviour under this assumption. However, an animal entering a new environment has incomplete knowledge of these parameters. If the predic- tions of optimal foraging theory are to hold the animal must use a behavioural rule which both learns the parameters and optimally exploits what it has learnt. In most circumstances it is not obvious that there exists any simple rule which has both these properties. We consider an environ- ment composed of well-defined patches of food, with each patch giving a smooth decelerating flow of food (Charnov, 1976). We present a simple rule which (asymptotically) learns about and optimally exploits this environment. We also show the rule can be modified to cope with a changing environment. We discuss what is meant by optimal behaviour in an unknown and possibly changing environment, using the simple rule we have presented for illustrative purposes.},
author = {McNamara, John M and Houston, Alasdair I},
doi = {10.1016/S0022-5193(85)80219-8},
file = {:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/McNamara, Houston - 1985 - Optimal foraging and learning.pdf:pdf},
isbn = {0022-5193},
issn = {00225193},
journal = {Journal of Theoretical Biology},
number = {2},
pages = {231--249},
title = {{Optimal foraging and learning}},
volume = {117},
year = {1985}
}
@article{Mcnamara2011,
abstract = {We propose operational definitions of reproductive optimism and pessimism; optimism involves behaving in a way that gives too much weight (in terms of producing surviving offspring) to positive events, pessimism gives too much weight to negative events. Natural selection maximizes the long-term growth of a lineage rather than short-term measures such as numbers of offspring. Consequently, optimism or pessimism can be favoured by natural selection, even though such biases appear irrational from a short-term perspective. We investigate the evolution of optimism in a metapopulation. The circumstances of a patch change over time, independently of other patches. With sufficient dispersal between patches, stochasticity affects members of a lineage largely independently and optimism is favoured. With little dispersal, the temporal fluctuations of a patch affect many members similarly; pessimism is then favoured. Our results establish that the spatial and temporal structure of the environment is crucial in determining the direction of evolved biases.},
annote = {The purpose of this paper is to determine whether optimism or pessimism are adaptive in a chaging environment. 'Optimism' and 'pessimism' refer to an individual's bias in its default estimate of the environment state.

One example is that of an individual in an environment that is expected to be either hot or cold. The individual does not 'know' in advance what the state of the environment will be. The individual's phenotype can be developed to offer varying degrees of insulation. But too much of it in a hot environment or too little in a cold one will result in death.

Given the above, the questions is whether a population of individuals should evolve to reflect the true probability that the environment is in one state.

A model is proposed where an individual lives in an environment that can be either in state A or B. The individual has a trait denoted by u and the individual's expected number of offspring in environment A and B is denoted by r{\_}A(u) and r{\_}B(u), respectively. The actual probability that the environment is in state B is denoted by p.

Box 1 defines the mean number of offspring left by an individual, given its trait, u, and the probability of environment state B, denoted by p. This mean is denoted by W(p,u). The fitness of the individual is defined as this mean, given the individual's trait, u, and the rate of indivivuals with the same trait in environment B. This rate is denoted by p(u) and thd individual's fitness is gien by W(p(u),u).

In the end the model serves to show that apparent irrational behaviour is favoured by natural selection. 'Irrational' in this case means a behaviour that does not maximise a short-term interest. In the model, two currencies pertain to an individual. The first is the number of offspring which is short-term. The second is fitness, which is long term. In the model the second currency does not necessarily require the other. Indeed, there is a trade-off between the two. Therefore fitness is not simply a matter of leaving as many offspring as possible. Thus individuals do not necessarily evolve to maximise their number of offspring. This evolved behaviour is what the authors call 'irrational.'


In the model, fitness is denoted as W(p,u) and defined as a function of the probability of the environment being hot (i.e., p) and the level of insulation of the individual (i.e., u).

Given this definition of fitness there are two probabilities to consider. The true probability that the environment is hot is denoted by p*. This is the objective probability. For an individual with trait u0, the probability that would maximise its fitness is denoted by p{\_}S. This is the subjective probability.

The individual's 'optimism' or 'pessimism' depends on whether the subjective probability is above or below the objective probability.

Since there are only two possible environments, an individual's subjective probability implicity indicates which environment the individual 'thinks' is most likely. That is to say, the individual 'thinks' that B occurs with probability p{\_}S (the subjective probability) and that A occurs with probability 1 - p{\_}S.

At any time, one environment, either A or B, provides the best reproduction rate to the individual. Thus this reproduction-maximising environment is the 'good' environment. 

If the individual 'thinks' that the 'good' environment is more likely than it objectively is, then the individual is optimistic.

If the individual 'thinks' that the 'good' environment is less likely than it objectively is, then the individual is pessimistic.

Example, if B is the good environment AND p{\_}S {\textgreater}p* then the individual is optimistic.

If A is the good environment AND (1-p{\_}S) {\textgreater} (1 - p*) then the individual is optimistic.

If B is the good environment AND p{\_}S {\textless} p* then the individual is pessimistic.

If A is the good environment AND (1-p{\_}S) {\textless} (1-p*) then the individual is pessimistic.


When there is spatial variation and no temporal variation (i.e., patches remain in the same state, either hot or cold) and most patches are hot or most patches are cold then individuals evolve to become optimistic. When most patches are cold individuals evolve *less* insulation than would maximise their number of offspring. On the other hand when most patches are hot then individuals evolve *more* insulation than would maximise their number of offspring. That is to say, individuals do not evolve to maximise their number of offspring.

Results are different when temporal variation in a single patch is considered instead. In this case pessimism evolves. That is to say, individuals evolve to overestimate the probability of a bad environment.},
author = {McNamara, John M and Trimmer, Pete C and Eriksson, A and Marshall, James A R and Houston, Alasdair I},
doi = {10.1111/j.1461-0248.2010.01556.x},
file = {:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/McNamara et al. - 2011 - Environmental variability can select for optimism or pessimism.pdf:pdf},
issn = {1461-0248},
journal = {Ecology Letters},
keywords = {Animals,Biological,Biological Evolution,Environment,Genetic,Models,Population Dynamics,Reproduction,Selection},
month = {jan},
number = {1},
pages = {58--62},
pmid = {21070564},
title = {{Environmental variability can select for optimism or pessimism}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21070564 http://onlinelibrary.wiley.com/doi/10.1111/j.1461-0248.2010.01556.x/full},
volume = {14},
year = {2011}
}
