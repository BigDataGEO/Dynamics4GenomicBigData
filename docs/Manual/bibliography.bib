@article{Frankenhuis2011a,
abstract = {Development is typically a constructive process, in which phenotypes incrementally adapt to local ecologies. Here, we present a novel model in which natural selection shapes developmental systems based on the evolutionary ecology, and these systems adaptively guide phenotypic development. We assume that phenotypic construction is incremental and trades off with sampling cues to the environmental state. We computed the optimal developmental programmes across a range of evolutionary ecological conditions. Using these programmes, we simulated distributions of mature phenotypes. Our results show that organisms sample the environment most extensively when cues are moderately, not highly, informative. When the developmental programme relies heavily on sampling, individuals transition from sampling to specialization at different times in ontogeny, depending on the consistency of their sampled cue set; this finding suggests that stochastic sampling may result in individual differences in plasticity itself. In addition, we find that different selection pressures may favour similar developmental mechanisms, and that organisms may incorrectly calibrate development despite stable ontogenetic environments. We hope our model will stimulate adaptationist research on the constructive processes guiding development.},
annote = {The authors present a model of the evolution of phenotypic plasticity.

Individuals may live in one of two environments: 1 or 0. Phenotypes can be either: 1 or 0. Matching between phenotype and environment determines the fitness of the individual.

The lifetime of the individual is composed of 20 discrete steps. In each step one of two actions is performed: sampling or specialising.

With sampling the individual gains noisy information regarding the environment it lives in. By specialising the individual modifies its phenotype according to its current belief. The two tasks are mutually exclusive in each life step.

The las two of the three methods to measure fitness are unclear to me (pag 3559, column 2, paragraph 2, highlighted). Why in 'diminishing returns' the fitness decreases with each step towards the correct environment assumption? What does the third one mean?

Results: If cues are moderately or highly informative then individuals evolve plasticity. When validities (the probability that the cue is truthful) are intermediate then the greatest reliance on sampling is observed. When cues are weakly informative then more sampling is required by each individual.

Plasticity decreases as one environment becomes more likely than the other (obvioulsy). Plasticity increases as cue validity increases.},
author = {Frankenhuis, Willem E and Panchanathan, Karthik},
doi = {10.1098/rspb.2011.0055},
file = {:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Frankenhuis, Panchanathan - 2011 - Balancing sampling and specialization an adaptationist model of incremental development.pdf:pdf;:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Frankenhuis, Panchanathan - 2011 - Balancing sampling and specialization an adaptationist model of incremental development(2).pdf:pdf;:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Frankenhuis, Panchanathan - 2011 - Balancing sampling and specialization an adaptationist model of incremental development(3).pdf:pdf;:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Frankenhuis, Panchanathan - 2011 - Balancing sampling and specialization an adaptationist model of incremental development(4).pdf:pdf},
issn = {1471-2954},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Biological,Biological Evolution,Environment,Genetic,Growth and Development,Models,Phenotype,Selection,Stochastic Processes},
