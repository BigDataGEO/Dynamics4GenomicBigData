file = {:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pallier et al. - 2002 - The role of individual differences in the accuracy of confidence judgments.pdf:pdf},
