@article{Vasas2012,
abstract = {Our current understanding of evolution is so tightly linked to template-dependent replication of DNA and RNA molecules that the old idea from Oparin of a self-reproducing 'garbage bag' ('coacervate') of chemicals that predated fully-fledged cell-like entities seems to be farfetched to most scientists today. However, this is exactly the kind of scheme we propose for how Darwinian evolution could have occurred prior to template replication.},
annote = {Main objective of this document: to outline the requirements to enable reaction networks to act as units of evolution, whatever the underlying chemistry used. Their claim is that autocatalytic sets themselves are not units of evolution.

The authors favour Oparin's coacervate model as a hypothesis of the origin of life, as opposed to the RNA-world (as the earliest life phneomenon) hypothesis, for example.

The authors claim that the minimum requirement is the presence of cores (essentially catalytic hypercycles) and the existence of two or more attractors but always with enough stability to converge to only one of them. Networks that only have one or zero attractors and those who have two or more but never converge to any of them can never be units of evolution.

Core: Members of a hypercycle. The authors describe it as a genotype.
Peripehry: Species whose replication is catalysed by member of a core. The authors describe it as a phenotype.
Suicidal autocatalyst: one that use only reactants produced by the autocatalytic reaction itself. It can be seen as a inhibitive set of reactions (A + X {\textless}-{\textgreater} 2A) where exponential growth never occurs. Catalysts that are not suicidal are called viable and these are the ones required -according to this paper- for the evolvability of a set.

The authors stress the idea pointed out by Eigen in the sense that in a cycle (or loop) it suffices that only one of the steps is actually catalytic for replication to take place. So when only some members of the core are intially present (with enough food), the whole core and periphery can be recreated.

The authors stress the idea that the growth of a reaction network is linked to the catalytic probability of the system. They mention results of Hordjik and Steel including the hypothesis that when the probability of catalysis of molecules (the probability that a molecule is able to catalyse any given reaction) reaches some threshold, a chain reaction is triggered and due to catalytic closure autocatalytic sets appear. Hordjina dn Steel tested this claim by generating random networks of reversible ligation/cleavage reactions between molecules of length up 20 where each had a probability P of catalysing a reaction.

Additionaly the authors test (p7) the system with a parameter K, representing the inhibition level of each molecule (inhibition meant as the total elimination of one molecule by another) but they conclude that inhibition does not qualitatively prevent the formation of large catalytic reaction networks.

*******************************
Experiment 1: Emergence of a catalytic set in a chemical network
An experiment is made with a chemical population in order to search for autocatalytic sets. Parameters of the simulation are the maximum length of the polymers in the set (M) and the probability that a molecule catalyses a reaction (P). When this two parameters become higher it becomes more likely that the number of reactions and different species increase beyond a threshold (Fig 3, A). A problem was detected since an increase in the maximum length of polymers results in a faster growth of the number of reactions potentially rendering ALL molecules as catalytic, which is deemed by the authors are utterly unrealistic.
*******************************
Experiment 2: evolvability of an autocatalytic set
The network is divided in compartments. Fitness is defined as the total (non-food species) mass of the compartment. Reproduction occurs by division of the compartments into propagules. When a compartment reaches a threshold size, it divides and the two daughter propagules replace their parent and another compartment that is randomly selected, in a Moran process.

The evolvability of the population of compartments can be ensured by providing "rare reactions", i.e., rare (possibly random) uncatalysed reactions involving food species where one of the non-food species is produced triggering the production of others within the same core and the ones in the periphery. Mutation is then introduced by (1) "rare reactions" that alter randomly (but midly) the contents of each compartment and (2) compartment division may yield in a daughter compartment where some loss of species occurs.

Conclusions: networks with only one attractor can never be units of evolution. Neither can molecular networks that never converge.

Conclusion {\#}2: For a catalytic network to be evolvable, it needs to be compartmentalized, and have several attractors but converge to only one.},
author = {Vasas, Vera and Fernando, Chrisantha and Santos, Mauro and Kauffman, Stuart and Szathm{\'{a}}ry, E{\"{o}}rs},
doi = {10.1186/1745-6150-7-1},
file = {:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Vasas et al. - 2012 - Evolution Before Genes.pdf:pdf},
issn = {1745-6150},
journal = {Biology direct},
keywords = {Adaptation,Biocatalysis,Biogenesis,Biological,Biological Evolution,Chemical,Computer Simulation,Genes,Genetic,Genotype,Models,Mutation,Phenotype,Polymers,Polymers: chemistry,Selection,Templates},
month = {jan},
number = {1},
pages = {1; discussion 1},
pmid = {22221860},
publisher = {BioMed Central Ltd},
title = {{Evolution Before Genes}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3284417{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {7},
year = {2012}
}
@article{Vasas2010,
abstract = {A basic property of life is its capacity to experience Darwinian evolution. The replicator concept is at the core of genetics-first theories of the origin of life, which suggest that self-replicating oligonucleotides or their similar ancestors may have been the first "living" systems and may have led to the evolution of an RNA world. But problems with the nonenzymatic synthesis of biopolymers and the origin of template replication have spurred the alternative metabolism-first scenario, where self-reproducing and evolving proto-metabolic networks are assumed to have predated self-replicating genes. Recent theoretical work shows that "compositional genomes" (i.e., the counts of different molecular species in an assembly) are able to propagate compositional information and can provide a setup on which natural selection acts. Accordingly, if we stick to the notion of replicator as an entity that passes on its structure largely intact in successive replications, those macromolecular aggregates could be dubbed "ensemble replicators" (composomes) and quite different from the more familiar genes and memes. In sharp contrast with template-dependent replication dynamics, we demonstrate here that replication of compositional information is so inaccurate that fitter compositional genomes cannot be maintained by selection and, therefore, the system lacks evolvability (i.e., it cannot substantially depart from the asymptotic steady-state solution already built-in in the dynamical equations). We conclude that this fundamental limitation of ensemble replicators cautions against metabolism-first theories of the origin of life, although ancient metabolic systems could have provided a stable habitat within which polymer replicators later evolved.},
author = {Vasas, Vera and Szathm{\'{a}}ry, E{\"{o}}rs and Santos, Mauro},
doi = {10.1073/pnas.0912628107},
file = {:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Vasas, Szathm{\'{a}}ry, Santos - 2010 - Lack of evolvability in self-sustaining autocatalytic networks constraints metabolism-first scenarios.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Biocatalysis,Biogenesis,Biological Evolution,Biopolymers,Biopolymers: metabolism,Gene Duplication,Genetic Fitness},
month = {jan},
number = {4},
pages = {1470--5},
pmid = {20080693},
title = {{Lack of evolvability in self-sustaining autocatalytic networks constraints metabolism-first scenarios for the origin of life.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2824406{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {107},
year = {2010}
}
