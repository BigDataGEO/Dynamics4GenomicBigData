author = {McNamara, John M},
doi = {10.1016/0040-5809(82)90018-1},
file = {:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/McNamara - 1982 - Optimal patch use in a stochastic environment.pdf:pdf},
