isbn = {00405809},
issn = {00405809},
journal = {Theoretical Population Biology},
keywords = {Archetypal model,Consumer-resource,Food-dependent individual growth,Population dynamics,Population size-structure,Stage-structured model},
number = {1},
pages = {47--62},
pmid = {18006030},
title = {{Simplifying a physiologically structured population model to a stage-structured biomass model}},
volume = {73},
year = {2008}
}
