file = {:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/De Nart, Tasso - 2014 - A personalized concept-driven recommender system for scientific libraries.pdf:pdf},
isbn = {18770509},
