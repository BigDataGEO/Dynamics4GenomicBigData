@article{Hammill2008,
abstract = {Phenotypic plasticity is one major source of variation in natural populations. Inducible defences, which can be considered threshold traits, are a form of plasticity that generates ecological and evolutionary consequences. A simple cost-benefit model underpins the maintenance and evolution of these threshold, inducible traits. In this model, a rank-order switch in expected fitness, defined by costs and benefits of induction between defended and undefended morphs, predicts the risk level at which individuals should induce defences. Here, taking predator-induced morphological defences in Daphnia pulex as a threshold trait, we provide the first comprehensive investigation into the costs and benefits of a threshold trait, and how they combine to reflect fitness and predict the switchpoint at which induction should occur. We develop reaction norms that show genetic variation in switchpoints. Further experiments show that induction can confer a survival benefit and a cost in terms of lifetime reproductive success. Together, these two traits combine to estimate expected fitness and can predict the switchpoint between an undefended and a defended strategy. The predictions match the reaction norm data for clones that experience these costs and benefits, and correspond well to independent field data on induction. However, predictions do not, and cannot, match for clones that do not gain a benefit from induction. This study confirms that a simple theory, based on life history costs and benefits, is a sufficient framework for understanding the ecology and evolution of inducible, threshold traits.},
annote = {The article aims to answer the following question: given two morphologies, defended and undefended, and varying levels of induced reactivity, what are the threshold conditions for which it is more evolutionarily stable to switch to a more sensitive induced strategy (i.e., defend only when exposed to the appropriate stimuli)?

The conditions include costs and benefits of induction as well as the mortality risk involved.

The switchpoint in terms of risk is where the fitness of both strategies are equal.

The authors present a model to predict this and to compare the predictions with experimental data.

Experimentally, the water fleas are exposed to stimuli from the predators and their reaction (development of pedestals and spikes) is measured for each concentration of predators.

Reaction norms ({\%} induction vs cue concentration) are taken.



The model shows the following:

1. There is genetic variation in the level of induction.
2. The defences indeed improve the individual's survival against predators.
3. Inducement of the defences (caused by exposure to predator cues) significantly decreases the individual's lifetime reproductive success (LRS).
4. The threshold transition from undefended to defenced can be predicted by measuring fitness as the product of survival and LRS.

Two clones were tested: cyril and colin.

The model predicts that cyril individuals should evolve to induce even when predator density is low and the experimental data agree with this prediction.

The model predicts that colin individuals should evolve to never induce in any of the experimental predator densities considered. The model shows that these individuals require predator densities as high as 100 times the density observed in the ponds the authors sampled. The reason for this is that in colin individuals induction results in increased mortality and unchanged reproductive rate. The authors explain that as long as inducement of the defences leads to reduced life expectanct the model can not make sensible predictions.

the authors argue that even though colin individuals might fail in induce when exposed to cues from Chaoborus, their small size might allow them to survive.

The low induction in colin is explained by the authors as follows. In their study, they have found out that fitness for both morphs (defended and undefended) is uniformly low and the difference in fitness between the two is very small. Therefore the defences do not offer a significant benefit in terms of fitness and colin individuals should have evolved low sensitivity to Cahoborus in order to avoid the costs incurred into when inducing the defences.},
author = {Hammill, E and Rogers, A and Beckerman, Andrew P},
doi = {10.1111/j.1420-9101.2008.01520.x},
file = {:C$\backslash$:/Users/jramirezidarraga/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hammill, Rogers, Beckerman - 2008 - Costs, benefits and the evolution of inducible defences a case study with Daphnia pulex.pdf:pdf},
issn = {1420-9101},
journal = {Journal of Evolutionary Biology},
keywords = {Adaptation,Animals,Biological Evolution,Daphnia,Daphnia: genetics,Daphnia: physiology,Diptera,Diptera: physiology,Physiological,Predatory Behavior},
month = {may},
number = {3},
pages = {705--15},
pmid = {18355186},
title = {{Costs, benefits and the evolution of inducible defences: a case study with Daphnia pulex}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18355186},
volume = {21},
year = {2008}
}
