issn = {01679473},
journal = {Computational Statistics and Data Analysis},
keywords = {Bayesian Lasso,Generalized linear mixed model,Metropolis-within-Gibbs algorithm,Probit mixed regression model,Ridge parameter,Stochastic search variable selection,Zellner prior},
